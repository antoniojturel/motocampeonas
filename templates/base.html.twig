<!DOCTYPE html>
<html lang="es">
  <head>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-63823164-1"></script>
    <script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());
    gtag('config', 'UA-63823164-1');
    </script>
    <!-- -->
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1"/>
    <title>{% block title %}{% endblock %}</title>

    <link rel="icon" type="image/x-icon" href="favicon.ico">

    <script id='jQueryOnline' src="https://code.jquery.com/jquery-3.6.0.js"></script>
    <script id='jQuerylocal' src="JS/jquery-3.6.0.js"></script>
    
    <link id='cssBootstrapLocal' href='CSS/bootstrap.min.css' rel='stylesheet'>
    <link id='cssBootstrapOnline' href='https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css' rel='stylesheet' integrity='sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3' crossorigin='anonymous'>
  </head>

  <body>
    {% block cabecera %}
      {{ include ('menu.html.twig') }}
    {% endblock %}

    {% block contenido %}
      <div class="container-fluid p-5 bg-light text-secondary text-center">
        <h1>{% block tituloContenido %}{% endblock %}</h1>
        <p>{% block subtituloContenido %}{% endblock %}</p> 
      </div>    
      {% block cuerpocontenido %}{% endblock %}
    {% endblock %}
    {% block pieDePagina %}
      {{ include ('pieDePagina.html.twig') }}
    {% endblock %}
    {% block cookies %}
      {{ include ('avisoCookies.html.twig') }}
      <script>
      $(document).ready(function(){
        $("#exampleModal").modal('show');
      });
      </script>
    {% endblock %}
    <script id='PopperJSlocal' src="JS/popper.min.js"></script> 
    <script id='PopperJSOnline' src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.10.2/dist/umd/popper.min.js" integrity="sha384-7+zCNj/IqJ95wo16oMtfsKbZ9ccEh31eOz1HGyDuCQ6wgnyJNSYdrPa03rtR1zdB" crossorigin="anonymous"></script>
    {# <script id='BootstrapJSlocal' src="JS/bootstrap.bundle.min.js"></script> #}
    <script id='BootstrapJSonline' src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.min.js" integrity="sha384-QJHtvGhmr9XOIpI6YVutG+2QOK9T+ZnN4kzFN1RtK3zEFEIsxhlmWl5/YESvpZ13" crossorigin="anonymous"></script>
  </body>

</html>